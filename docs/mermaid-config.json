{
  "theme": "dark",
  "themeVariables": {
    "fontFamily": "Arial, sans-serif",
    "fontSize": "14px",
    "primaryColor": "#1a1a1a",
    "primaryTextColor": "#ffffff",
    "primaryBorderColor": "#64b5f6",
    "lineColor": "#90a4ae",
    "sectionBkgColor": "#2d2d2d",
    "altSectionBkgColor": "#1a1a1a",
    "gridColor": "#424242",
    "background": "#0d1117",
    "mainBkg": "#0d1117",
